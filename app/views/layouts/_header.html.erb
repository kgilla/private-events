<header>
  <div class="container">
    <h1><%= link_to "Private Events", root_path %></h1>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if @current_user.nil? %>
          <li><%= link_to "Sign Up", '/users/new' %></li>
          <li> <%= link_to "Log In", login_path %></li>
          <li class="divider"></li>
        <% else %>
          <li> <%= link_to "All Events", "/events" %></li>
          <li> <%= link_to "My Events", "/users/#{current_user.id}" %></li>
          <li> <%= link_to "Log Out", logout_path %></li>  
          <li class="divider"></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
