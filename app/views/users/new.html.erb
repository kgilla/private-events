<div class="form">
<h1>Make a new user</h1>
<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.label :username %><br />
    <%= f.text_field :username, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :email %><br />
    <%= f.text_field :email, class: "form-control" %>
  </div>
    <div><%= submit_tag "Create", class: "btn btn-primary btn-lg" %></div>
<% end %>
</div>
